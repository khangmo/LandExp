<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.5.xsd">
    <!--
        Added the constraints for entity LandProjects.
    -->
    <changeSet id="20180607131202-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="city_id"
                                 baseTableName="land_projects"
                                 constraintName="fk_land_projects_city_id"
                                 referencedColumnNames="id"
                                 referencedTableName="city"/>

        <addForeignKeyConstraint baseColumnNames="street_id"
                                 baseTableName="land_projects"
                                 constraintName="fk_land_projects_street_id"
                                 referencedColumnNames="id"
                                 referencedTableName="street"/>

        <addForeignKeyConstraint baseColumnNames="create_by_id"
                                 baseTableName="land_projects"
                                 constraintName="fk_land_projects_create_by_id"
                                 referencedColumnNames="id"
                                 referencedTableName="jhi_user"/>

        <addForeignKeyConstraint baseColumnNames="update_by_id"
                                 baseTableName="land_projects"
                                 constraintName="fk_land_projects_update_by_id"
                                 referencedColumnNames="id"
                                 referencedTableName="jhi_user"/>

    </changeSet>
</databaseChangeLog>
